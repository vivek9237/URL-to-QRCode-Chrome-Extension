{
    "name": "URL to QRCode",
    "version": "1.0",
    "description": "Convert URL to QR Code",
    "manifest_version": 2,
	"permissions": ["tabs"],
	"background": {
        "scripts": [
            "jquery.min.js","background.js","qrcode.js"
        ],
        "persistent": false
    },
	"browser_action": {
        "default_title": "URL to QRCode",
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    }
}